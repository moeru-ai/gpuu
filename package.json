{
  "name": "gpuu",
  "type": "module",
  "version": "1.0.3",
  "packageManager": "pnpm@10.12.1",
  "description": "A collection of utilities for working with GPUs, especially for WebGPU.",
  "author": {
    "name": "Neko Ayaka",
    "email": "neko@ayaka.moe",
    "url": "https://github.com/nekomeowww"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moeru-ai/gpuu.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    },
    "./webgpu": {
      "types": "./dist/webgpu/index.d.ts",
      "default": "./dist/webgpu/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "pnpm run stub",
    "stub": "unbuild --stub",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "dependencies": {
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@types/node": "^22.15.31",
    "@webgpu/types": "^0.1.61",
    "bumpp": "^10.1.1",
    "eslint": "^9.29.0",
    "eslint-plugin-import-x": "^4.15.2",
    "taze": "^19.1.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  }
}
